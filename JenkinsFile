pipeline {
    agent any
    tools {
        maven 'Maven' // Make sure Maven is installed and named 'Maven' in Jenkins
    }
    stages {
        stage('Build') {
            steps {
                sh 'mvn clean compile'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Docker') {
          steps {
               sh 'docker image ls'
          }
        }
    }
}